extends Node3D


# ------------------------------------------------------------------------------
# Onready Variables
# ------------------------------------------------------------------------------
@onready var _dungeon : Node3D = $Dungeon

# ------------------------------------------------------------------------------
# Override Methods
# ------------------------------------------------------------------------------
func _ready() -> void:
	if CrawlGlobals.Load_Config() != OK:
		CrawlGlobals.Reset_Config()
		CrawlGlobals.Save_Config()
	
	if _dungeon.load_dungeon("user://map.tres") != OK:
		_dungeon.create_default()


